{(
    Nil = Object subclass,
    Nil extend: (
        #& an_object => false,
        #| an_object => an_object,
        #^ an_object => self xor: an_object,

        #inspect => "nil",
        #is_nil? => true,

        #to_c => '',
        #to_f => 0.0,
        #to_h => {},
        #to_i => 0,
        #to_t => [],
        #to_s => ""
    )
)}
