(
    $Margaret bind: #self => @self,
    $Margaret bind: #super => @super,
    $Margaret bind: #delegated => 42,

    $Parent = $Margaret clone: "$Parent",
    $Parent bind: #self => @self,
    $Parent bind: #super => @super,

    $Child = $Parent clone: "$Child",
    $Child bind: #self => @self,
    $Child bind: #super => @super,

    puts: $Margaret delegated,
    puts: $Parent delegated,
    puts: $Child delegated,

    puts: $Child notboundmessage,
)
