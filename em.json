{
  "name": "margaret",

  "version": "0.0.1",

  "dependencies": {
    "EmeraldsBool": "Oblivious-Oblivious/EmeraldsBool",
    "EmeraldsCrossline": "Oblivious-Oblivious/EmeraldsCrossline",
    "EmeraldsVector": "Oblivious-Oblivious/EmeraldsVector"
  },

  "dev-dependencies": {
    "cSpec": "Oblivious-Oblivious/cSpec"
  },

  "build": "",

  "compile-flags": {
    "cc": "clang",
    "debug": {
      "opt": "-Og -g",
      "version": "-std=c99",
      "flags": "-Wall -Wextra -Werror -pedantic -pedantic-errors -Wpedantic",
      "warnings": "-Wno-unused-function -Wno-strict-prototypes"
    },
    "release": {
      "opt": "-O3",
      "version": "-std=c2x",
      "flags": "",
      "warnings": ""
    },
    "test": {
      "opt": "-Og -g",
      "version": "-std=c99",
      "flags": "",
      "warnings": ""
    }
  },

  "license": "gpl-v3"
}
