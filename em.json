{
  "name": "margaret",

  "version": "0.0.1",

  "dependencies": {
    "EmeraldsBool": "The-Fourth-Wall/EmeraldsBool",
    "EmeraldsCrossline": "The-Fourth-Wall/EmeraldsCrossline",
    "EmeraldsFastStack": "The-Fourth-Wall/EmeraldsFastStack",
    "EmeraldsHashtable": "The-Fourth-Wall/EmeraldsHashtable",
    "EmeraldsOniguruma": "The-Fourth-Wall/EmeraldsOniguruma",
    "EmeraldsReadHandler": "The-Fourth-Wall/EmeraldsReadHandler",
    "EmeraldsString": "The-Fourth-Wall/EmeraldsString",
    "EmeraldsVector": "The-Fourth-Wall/EmeraldsVector"
  },

  "dev-dependencies": {
    "cSpec": "Oblivious-Oblivious/cSpec"
  },

  "build": "",

  "compile-flags": {
    "cc": "clang",
    "debug": {
      "opt": "-Og -g",
      "version": "-std=c11",
      "flags": "-Wall -Wextra -Werror -pedantic -pedantic-errors -Wpedantic",
      "warnings": "-Wno-extra-semi -Wno-unused-function -Wno-strict-prototypes"
    },
    "release": {
      "opt": "-O3",
      "version": "-std=c2x",
      "flags": "",
      "warnings": ""
    },
    "test": {
      "opt": "-Og -g",
      "version": "-std=c11",
      "flags": "",
      "warnings": ""
    }
  },

  "license": "gpl-v3"
}
